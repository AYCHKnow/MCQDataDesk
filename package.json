{
  "name": "Assessorator",
  "version": "0.1.0",
  "description": "An assessment generator application.",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "pack": "electron-packager . Assessorator --all --out=bin --ignore=bin --asar --overwrite",
    "buildWindowsInstallerOld": "electron-builder bin/Assessorator-win32-x64 --platform=win --out=bin",
    "createInstaller": "grunt create-windows-installer"
  },
  "author": "Matt Lyons <matt@matt-lyons.net>",
  "dependencies": {
    "appdirectory": "~0.1.0",
    "mkdirp": "~0.5.1"
  },
  "devDependencies": {
    "electron-prebuilt": "^0.36.0",
    "electron-packager": "~5.2.1",
    "electron-builder": "~2.7.2",
    "grunt-electron-installer": "~1.2.2",
    "grunt": "~0.4.5"
  },
  "builder": {
    "win": {
      "title": "Assessorator",
      "icon": "icon.ico",
      "version": "0.1.0",
      "publisher": "Matt Lyons <matt@matt-lyons.net>"
    }
  }
}