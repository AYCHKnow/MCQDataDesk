{
  "name": "Assessorator",
  "version": "0.7.0",
  "description": "An application designed manage multiple choice question banks, generate assessments and export in edX format.",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "pack": "electron-packager . Assessorator --all --out=bin --ignore=bin --asar --overwrite",
    "buildWindowsInstallerOld": "electron-builder bin/Assessorator-win32-x64 --platform=win --out=bin",
    "createInstaller": "grunt create-windows-installer"
  },
  "author": "Matt Lyons <matt@matt-lyons.net>",
  "dependencies": {
    "appdirectory": "~0.1.0",
    "mkdirp": "~0.5.1",
    "md5": "~2.2.1"
  },
  "devDependencies": {
    "electron": "^1.4.6",
    "electron-packager": "~7.7.0",
    "electron-builder": "~6.3.0",
    "grunt-electron-installer": "~2.1.0",
    "grunt": "~1.0.1",
    "devtron": "^1.3.0"
  },
  "build": {
    "win": {
      "title": "Assessorator",
      "icon": "icon.ico",
      "version": "0.7.0",
      "publisher": "Matt Lyons <matt@matt-lyons.net>"
    }
  }
}